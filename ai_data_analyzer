    {
      "cell_type": "code",
      "source": [
        "%%writefile data_analyzer.py\n",
        "import pandas as pd\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "def load_data():\n",
        "    return pd.read_csv(\"sales_data.csv\")\n",
        "\n",
        "def analyze_data(df):\n",
        "    print(\"\\n--- SUMMARY STATISTICS ---\")\n",
        "    print(df.describe())\n",
        "\n",
        "    print(\"\\n--- TOTAL SALES PER MONTH ---\")\n",
        "    print(df.groupby(\"Month\")[\"Total_Sales\"].sum())\n",
        "\n",
        "    print(\"\\n--- TOTAL SALES PER PRODUCT ---\")\n",
        "    print(df.groupby(\"Product\")[\"Total_Sales\"].sum())\n",
        "\n",
        "def visualize(df):\n",
        "    df.groupby(\"Month\")[\"Total_Sales\"].sum().plot(kind=\"bar\")\n",
        "    plt.title(\"Monthly Total Sales\")\n",
        "    plt.show()\n",
        "\n",
        "    df.groupby(\"Product\")[\"Total_Sales\"].sum().plot(kind=\"pie\", autopct=\"%1.1f%%\")\n",
        "    plt.title(\"Sales By Product\")\n",
        "    plt.show()\n",
        "\n",
        "if __name__ == \"__main__\":\n",
        "    df = load_data()\n",
        "    analyze_data(df)\n",
        "    visualize(df)\n"
      ],
